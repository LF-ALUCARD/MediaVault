package devfull.MediaVault.entities.enums;

public enum UserRole {

	ADMIN(0),
	USER(1);
	
	private int code;
	
	public UserRole(int code) {
		this.code = code;
	}
	
	public int getCode() {
		return this.code;
	}
	
	public static UserRole valueOfCode(int code) {
		for(UserRole x : UserRole.values()) {
			if (x.getCode() == code) {
				return code;
			}
		}
		throw new IllegalArgumentException("CÃ³dico invalido");
	}
}
